<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dreammer"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.dreammer.MainActivity"
            android:label="@string/app_name" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="But_now" android:screenOrientation="portrait"></activity>
        <activity android:name="How_dream" android:screenOrientation="portrait"></activity>
        <activity android:name="No_dream" android:screenOrientation="portrait"></activity>
        <activity android:name="What_dream" android:screenOrientation="portrait"></activity>
        <activity android:name="When_dream" android:screenOrientation="portrait"></activity>
    </application>

</manifest>
